<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.wenzhaung.projectcalculator" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/calculator"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CircleActivity"
            android:label="@string/title_activity_circle" >
        </activity>
        <activity
            android:name=".EllipseActivity"
            android:label="@string/title_activity_ellipse" >
        </activity>
        <activity
            android:name=".ArchActivity"
            android:label="@string/title_activity_arch" >
        </activity>
        <activity
            android:name=".TrapActivity"
            android:label="@string/title_activity_trap" >
        </activity>
    </application>

</manifest>
